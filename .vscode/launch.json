{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/services/product_catalog/server.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to product_catalog",
      "address": "localhost",
      "port": 9229,
      "sourceMapPathOverrides": {
        "/usr/src/app/*": "${workspaceRoot}/services/product_catalog/*"
      },
      "protocol": "inspector"
    }
  ]
}